{
  "name": "KAILASH",
  "short_name": "KAILASH",
  "description": "Traditional Elegance. Carefully curated collections sourced from trusted vendors.",
  
  "start_url": "/kailash-app/",
  "scope": "/kailash-app/",
  "id": "/kailash-app/",
  "display": "standalone",
  
  "background_color": "#232f3e",
  "theme_color": "#ff9900",
  "orientation": "portrait-primary",
  "lang": "en-US",
  "dir": "ltr",

  "icons": [
    {
      "src": "/kailash-app/icons/pic.png",
      "type": "image/png",
      "sizes": "192x192",
      "purpose": "any"
    },
    {
      "src": "/kailash-app/icons/pic.png",
      "type": "image/png",
      "sizes": "512x512",
      "purpose": "maskable"
    }
  ],

  "screenshots": [
    {
      "src": "/kailash-app/screenshots/screenshot1.png",
      "sizes": "1080x1920",
      "type": "image/png",
      "label": "Welcome to KAILASH"
    },
    {
      "src": "/kailash-app/screenshots/screenshot2.png",
      "sizes": "1080x1920",
      "type": "image/png",
      "label": "Browse our product collections"
    }
  ],
  
  "categories": [
    "shopping",
    "lifestyle",
    "fashion"
  ],

  "shortcuts": [
    {
      "name": "View Cart",
      "url": "/kailash-app/#cart",
      "icons": [{ "src": "/kailash-app/icons/icon-96x96.png", "sizes": "96x96" }]
    },
    {
      "name": "Admin Panel",
      "url": "/kailash-app/add-product.html",
      "icons": [{ "src": "/kailash-app/icons/icon-96x96.png", "sizes": "96x96" }]
    }
  ],

  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  
  "prefer_related_applications": false
}
